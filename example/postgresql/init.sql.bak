INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-46714', 'https://cve.circl.lu/cve/CVE-2024-46714', 'drm/amd/display: Skip wbscl_set_scaler_filter if filter is null', 0, 'unknown', e'In the Linux kernel, the following vulnerability has been resolved:

