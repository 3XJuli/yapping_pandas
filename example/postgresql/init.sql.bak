INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-46725', 'https://cve.circl.lu/cve/CVE-2024-46725', 'drm/amdgpu: Fix out-of-bounds write warning', 0, 'unknown', e'In the Linux kernel, the following vulnerability has been resolved:

