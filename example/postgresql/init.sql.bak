INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-46782', 'https://cve.circl.lu/cve/CVE-2024-46782', 'ila: call nf_unregister_net_hooks() sooner', 0, 'unknown', e'In the Linux kernel, the following vulnerability has been resolved:

