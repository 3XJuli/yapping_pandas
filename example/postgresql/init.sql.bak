INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-46790', 'https://cve.circl.lu/cve/CVE-2024-46790', 'codetag: debug: mark codetags for poisoned page as empty', 0, 'unknown', e'In the Linux kernel, the following vulnerability has been resolved:

