INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-46776', 'https://cve.circl.lu/cve/CVE-2024-46776', 'drm/amd/display: Run DC_LOG_DC after checking link->link_enc', 0, 'unknown', e'In the Linux kernel, the following vulnerability has been resolved:

