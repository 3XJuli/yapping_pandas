INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-44985', 'https://cve.circl.lu/cve/CVE-2024-44985', 'ipv6: prevent possible UAF in ip6_xmit()', 0, 'unknown', e'In the Linux kernel, the following vulnerability has been resolved:

