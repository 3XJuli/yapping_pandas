INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-46769', 'https://cve.circl.lu/cve/CVE-2024-46769', 'spi: intel: Add check devm_kasprintf() returned value', 0, 'unknown', e'In the Linux kernel, the following vulnerability has been resolved:

