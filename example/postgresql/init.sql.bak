INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-44988', 'https://cve.circl.lu/cve/CVE-2024-44988', 'net: dsa: mv88e6xxx: Fix out-of-bound access', 0, 'unknown', e'In the Linux kernel, the following vulnerability has been resolved:

net: dsa: mv88e6xxx: Fix out-of-bound access

