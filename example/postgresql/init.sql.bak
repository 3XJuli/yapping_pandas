INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-3673', 'https://cve.circl.lu/cve/CVE-2024-3673', 'Web Directory Free < 1.7.3 - Unauthenticated LFI', 9.1, 'CRITICAL', 'The Web Directory Free WordPress plugin before 1.7.3 does not validate a parameter before using it in an include(), which could lead to Local File Inclusion issues.', '');
INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-39613', 'https://cve.circl.lu/cve/CVE-2024-39613', 'RCE in desktop app in Windows by local attacker', 0, 'unknown', 'Mattermost Desktop App versions <=5.8.0 fail to specify an absolute path when searching the cmd.exe file,  which allows a local attacker who is able to put an cmd.exe file in the Downloads folder of a user''s machine to cause remote code execution on that machine.', 'Design: Enforce principle of least privilege Design: Ensure that the program''s compound parts, including all system dependencies, classpath, path, and so on, are secured to the same or higher level assurance as the program Implementation: Host integrity monitoring');
INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-40770', 'https://cve.circl.lu/cve/CVE-2024-40770', null, 0, 'unknown', 'A permissions issue was addressed with additional restrictions. This issue is fixed in macOS Sequoia 15. A non-privileged user may be able to modify restricted network settings.', '');
INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-40825', 'https://cve.circl.lu/cve/CVE-2024-40825', null, 6, 'MEDIUM', 'The issue was addressed with improved checks. This issue is fixed in visionOS 2, macOS Sequoia 15. A malicious app with root privileges may be able to modify the contents of system files.', '');
INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-40838', 'https://cve.circl.lu/cve/CVE-2024-40838', null, 0, 'unknown', 'A privacy issue was addressed by moving sensitive data to a protected location. This issue is fixed in macOS Sequoia 15. A malicious app may be able to access notifications from the user''s device.', '');
INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-40840', 'https://cve.circl.lu/cve/CVE-2024-40840', null, 0, 'unknown', 'This issue was addressed through improved state management. This issue is fixed in iOS 18 and iPadOS 18. An attacker with physical access may be able to use Siri to access sensitive user data.', '');
INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-40848', 'https://cve.circl.lu/cve/CVE-2024-40848', null, 0, 'unknown', 'A downgrade issue was addressed with additional code-signing restrictions. This issue is fixed in macOS Ventura 13.7, macOS Sonoma 14.7, macOS Sequoia 15. An attacker may be able to read sensitive information.', '');
INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-40857', 'https://cve.circl.lu/cve/CVE-2024-40857', null, 0, 'unknown', 'This issue was addressed through improved state management. This issue is fixed in Safari 18, visionOS 2, watchOS 11, macOS Sequoia 15, iOS 18 and iPadOS 18, tvOS 18. Processing maliciously crafted web content may lead to universal cross site scripting.', 'Use browser technologies that do not allow client-side scripting. Utilize strict type, character, and encoding enforcement. Ensure that all user-supplied input is validated before use.');
INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-40860', 'https://cve.circl.lu/cve/CVE-2024-40860', null, 0, 'unknown', 'A logic issue was addressed with improved checks. This issue is fixed in macOS Sonoma 14.7, macOS Sequoia 15. An app may be able to modify protected parts of the file system.', '');
