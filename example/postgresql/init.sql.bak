INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-46779', 'https://cve.circl.lu/cve/CVE-2024-46779', 'drm/imagination: Free pvr_vm_gpuva after unlink', 0, 'unknown', e'In the Linux kernel, the following vulnerability has been resolved:

