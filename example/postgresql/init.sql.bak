INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-46788', 'https://cve.circl.lu/cve/CVE-2024-46788', 'tracing/osnoise: Use a cpumask to know what threads are kthreads', 0, 'unknown', e'In the Linux kernel, the following vulnerability has been resolved:

