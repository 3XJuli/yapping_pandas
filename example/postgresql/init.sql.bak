INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-44989', 'https://cve.circl.lu/cve/CVE-2024-44989', 'bonding: fix xfrm real_dev null pointer dereference', 0, 'unknown', e'In the Linux kernel, the following vulnerability has been resolved:

