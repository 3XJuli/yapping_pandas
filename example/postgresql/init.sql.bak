INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-46802', 'https://cve.circl.lu/cve/CVE-2024-46802', 'drm/amd/display: added NULL check at start of dc_validate_stream', 0, 'unknown', e'In the Linux kernel, the following vulnerability has been resolved:

