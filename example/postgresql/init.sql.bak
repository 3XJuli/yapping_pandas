INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-39574', 'https://cve.circl.lu/cve/CVE-2024-39574', null, 0, 'unknown', 'Dell PowerScale InsightIQ, version 5.1, contain an Improper Privilege Management vulnerability. A high privileged attacker with local access could potentially exploit this vulnerability, leading to Denial of service.', '');
INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-39583', 'https://cve.circl.lu/cve/CVE-2024-39583', null, 0, 'unknown', 'Dell PowerScale InsightIQ, versions 5.0 through 5.1, contains a Use of a Broken or Risky Cryptographic Algorithm vulnerability. An unauthenticated attacker with remote access could potentially exploit this vulnerability, leading to Elevation of privileges.', '');
INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-40654', 'https://cve.circl.lu/cve/CVE-2024-40654', null, 7.8, 'HIGH', 'In multiple locations, there is a possible permission bypass due to a confused deputy. This could lead to local escalation of privilege with no additional execution privileges needed. User interaction is needed for exploitation.', '');
INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-42423', 'https://cve.circl.lu/cve/CVE-2024-42423', null, 0, 'unknown', 'Citrix Workspace App version 23.9.0.24.4 on Dell ThinOS 2311 contains an Incorrect Authorization vulnerability when Citrix CEB is enabled for WebLogin. A local unauthenticated user with low privileges may potentially exploit this vulnerability to bypass existing controls and perform unauthorized actions leading to information disclosure and tampering.', '');
INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-43387', 'https://cve.circl.lu/cve/CVE-2024-43387', 'Phoenix Contact: Access files due to improper neutralization of special elements in MGUARD devices', 0, 'unknown', 'A low privileged remote attacker can read and write files as root due to improper neutralization of special elements in the variable EMAIL_RELAY_PASSWORD in mGuard devices.', 'Use language APIs rather than relying on passing data to the operating system shell or command line. Doing so ensures that the available protection mechanisms in the language are intact and applicable. Filter all incoming data to escape or remove characters or strings that can be potentially misinterpreted as operating system or shell commands All application processes should be run with the minimal privileges required. Also, processes must shed privileges as soon as they no longer require them.');
INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-43388', 'https://cve.circl.lu/cve/CVE-2024-43388', 'Phoenix Contact: SNMP reconfiguration due to improper input validation in MGUARD devices', 0, 'unknown', 'A low privileged remote attacker with write permissions can reconfigure the SNMP service due to improper input validation.', '');
INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-43454', 'https://cve.circl.lu/cve/CVE-2024-43454', 'Windows Remote Desktop Licensing Service Remote Code Execution Vulnerability', 0, 'unknown', 'Windows Remote Desktop Licensing Service Remote Code Execution Vulnerability', '');
