INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-46736', 'https://cve.circl.lu/cve/CVE-2024-46736', 'smb: client: fix double put of @cfile in smb2_rename_path()', 0, 'unknown', e'In the Linux kernel, the following vulnerability has been resolved:

smb: client: fix double put of @cfile in smb2_rename_path()

