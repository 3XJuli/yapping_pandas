INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-46775', 'https://cve.circl.lu/cve/CVE-2024-46775', 'drm/amd/display: Validate function returns', 0, 'unknown', e'In the Linux kernel, the following vulnerability has been resolved:

