INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-7834', 'https://cve.circl.lu/cve/CVE-2024-7834', 'Local privilege escalation in Overwolf', 0, 'unknown', e'A local privilege escalation is caused by Overwolf
