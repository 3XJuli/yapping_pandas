INSERT INTO public.vulnerability (cve, url, title, score, severity, description, solution) VALUES ('CVE-2024-40842', 'https://cve.circl.lu/cve/CVE-2024-40842', null, 0, 'unknown', 'An issue was addressed with improved validation of environment variables. This issue is fixed in macOS Sequoia 15. An app may be able to access user-sensitive data.', '');
