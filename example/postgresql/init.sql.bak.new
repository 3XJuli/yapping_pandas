INSERT INTO public.vulnerability (cve, published_at, url, title, score, severity, description, solution) VALUES ('CVE-2024-32668', '2024-11-23 22:19:00.510746', 'https://cve.circl.lu/cve/CVE-2024-32668', 'bhyve(8) privileged guest escape via USB controller', 8.2, 'HIGH', e'An insufficient boundary validation in the USB code could lead to an out-of-bounds write on the heap, with data controlled by the caller.

